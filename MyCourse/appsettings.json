{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=(localdb)\\MyLocalDB;Database=MyCourse;Trusted_Connection=True;"
  }
  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Title",
      "Ascending": true,
      "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  }
}
